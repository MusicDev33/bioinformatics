    {{ selector("2020-2021", routerYearRange) }}
    {{ selector("2017-2019", routerYearRange) }}
    {{ selector("2014-2016", routerYearRange) }}
    {{ selector("2011-2013", routerYearRange) }}
    {{ selector("2008-2010", routerYearRange) }}
    {{ selector("2005-2007", routerYearRange) }}
  </section>
</article>

