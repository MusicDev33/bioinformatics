{% macro selector(btnString) %}
  {% if btnString === routerYearRange %}
    <div class="col-auto">
      <a href="/publications?range={{ btnString }}" class="year-selector active">{{ btnString }}</a>
    </div>
  {% else %}
    <div class="col-auto">
      <a href="/publications?range={{ btnString }}" class="year-selector">{{ btnString }}</a>
    </div>
  {% endif %}
{% endmacro %}

